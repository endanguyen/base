{
  "name": "laravel-rocket/base",
  "description": "Base App Code For Laravel Rocket",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/laravel-rocket/foundation"
    },
    {
      "type": "vcs",
      "url": "https://github.com/laravel-rocket/generator"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.4.*",
    "laravel/socialite": "~3.0",
    "laravel/tinker": "~1.0",
    "aws/aws-sdk-php": "~3.24",
    "laravelcollective/html": "5.4.*",
    "laravel-rocket/foundation": "dev-master",
    "laravel-rocket/generator": "dev-master"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "~2.3",
    "barryvdh/laravel-ide-helper": "~2.3",
    "doctrine/dbal": "~2.5",
    "fzaninotto/faker": "~1.4",
    "laravel/browser-kit-testing": "^1.0",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
