{
  "tables": {
    "admin_users": {
      "relations": {
        "adminRoles": {
          "selection": "multiple",
          "target": "role"
        }
      }
    },
    "admin_user_roles": {
      "columns": {
        "role": {
          "type": "type",
          "options": [
            {
              "name": "Super User",
              "value": "super_user"
            },
            {
              "name": "Site Admin",
              "value": "site_admin"
            }
          ]
        }
      }
    },
    "files": {
      "columns": {
        "file_type": {
          "type": "type",
          "options": [
            {
              "name": "Image",
              "value": "image"
            },
            {
              "name": "Video",
              "value": "video"
            }
          ]
        },
        "storage_type": {
          "type": "type",
          "options": [
            {
              "name": "S3",
              "value": "s3"
            },
            {
              "name": "Local",
              "value": "local"
            },
            {
              "name": "URL",
              "value": "url"
            }
          ]
        }
      }

    }
  }
}
